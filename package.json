    "extract": "pnpm run-ts scripts/extract.ts",
    "export": "cross-env EXPORT_MODE=true next build && cross-env EXPORT_MODE=true next export && pnpm extract",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "run-ts": "node -r esbuild-register",
    "typecheck": "tsc"
  },
