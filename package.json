{
  "name": "@camome/saazy",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "gen-scss-type": "typed-scss-modules \"src/**/*.module.scss\" --aliasPrefixes.@ src --outputFolder .scss-types --nameFormat none --exportType default --logLevel error",
    "gen-theme": "camome theme --output src/styles/theme.css",
    "dev:next": "next dev --port 3930",
    "dev": "concurrently -n scss,next -c green,cyan \"pnpm gen-scss-type -w \" \"pnpm dev:next\"",
    "build": "next build",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/.eslint-cache --max-warnings=0 .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --cache --check .",
    "format:fix": "prettier --cache --write .",
    "typecheck": "tsc",
    "prepare": "husky install",
    "ci:check": "concurrently \"pnpm typecheck\" \"pnpm lint\" \"pnpm format\""
  },
  "dependencies": {
    "@camome/core": "^0.0.29",
    "@camome/system": "^0.1.24",
    "@headlessui/react": "^1.7.9",
    "@heroicons/react": "^2.0.10",
    "@mdx-js/loader": "^2.2.1",
    "@mdx-js/mdx": "^2.1.5",
    "@next/mdx": "^13.0.0",
    "body-scroll-lock": "4.0.0-beta.0",
    "clsx": "^1.2.1",
    "contentlayer": "^0.3.0",
    "github-slugger": "^2.0.0",
    "image-size": "^1.0.2",
    "lodash.sortby": "^4.7.0",
    "mdast-util-to-string": "^3.1.1",
    "mdx": "^0.3.1",
    "mdx-bundler": "^9.2.1",
    "next": "13.1.6",
    "next-contentlayer": "^0.3.0",
    "next-mdx-remote": "^4.3.0",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-merge-refs": "^2.0.1",
    "react-wrap-balancer": "^0.4.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.1",
    "rehype-pretty-code": "^0.9.3",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-code-titles": "^0.1.2",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.1.5",
    "remark-parse": "^10.0.1",
    "sharp": "^0.31.3",
    "shiki": "^0.14.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@camome/utils": "^0.1.10",
    "@svgr/webpack": "^6.5.1",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/jsdom": "^20.0.0",
    "@types/lodash.sortby": "^4.7.7",
    "@types/mdx": "^2.0.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "camome": "^0.1.21",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.31.10",
    "globby": "^13.1.2",
    "husky": "^8.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.58.0",
    "typed-scss-modules": "^7.0.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18"
  }
}
